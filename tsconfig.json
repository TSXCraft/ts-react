{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "strict": true,
    
    "moduleResolution": "bundler",
    
    "jsx": "react-jsx",
    "jsxImportSource": "@",

    "baseUrl": "./src",
    "paths": {
      "@core": ["core/index"],
      "@core/*": ["core/*"],
      "@/jsx-runtime": ["jsx-runtime/index"]
    }
  },
  "esModuleInterop": true,
  "skipLibCheck": true
}

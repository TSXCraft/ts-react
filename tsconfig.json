{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": ".",
    "moduleResolution": "bundler",
    "baseUrl": "./src",
    "paths": {
      "@core": ["core/index"],
      "@core/*": ["core/*"]
    }
  },
  "esModuleInterop": true,
  "skipLibCheck": true
}
